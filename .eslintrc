{
  "extends": "airbnb-base",
  "rules": {
    "strict": 0,
    "func-names": 0,
    "global-require": 0,
    "no-console": 0,
    "consistent-return": 0,
    "curly": 0,
    "no-shadow": [2, {
      "allow": ["err"]
    }],
    "no-underscore-dangle": [2, {
      "allow": ["_id"]
    }],
    "no-param-reassign": [2, {
      "props": false
    }],
    "import/no-unresolved": 0
  }
}
